<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans" xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config/>

    <bean id="dissTermsJson" class="de.qucosa.oai.provider.config.dao.DissTermsJson">
        <constructor-arg value="${config.path}/dissemination-config.json" name="path" />
    </bean>

    <bean id="setJson" class="de.qucosa.oai.provider.config.dao.SetJson">
        <constructor-arg name="path" value="${config.path}/list-set-conf.json" />
    </bean>

    <!--<bean id="jdbcTemplate" class="org.springframework.jdbc.core.JdbcTemplate" />-->

    <!--<bean id="setModel" class="de.qucosa.oai.provider.persitence.model.Set" />-->
</beans>